{
	"extends": [
		"airbnb",
		"airbnb-typescript",
		"prettier",
		"next/core-web-vitals"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "warn",
		"no-console": "off",
		"func-names": "off",
		"no-process-exit": "off",
		"object-shorthand": "off",
		"class-methods-use-this": "off",
		"node/no-missing-import": "off",
		"import/missing-extensions": "off",
		"import/prefer-default-export": "off"
	},
	"parserOptions": {
		"project": "./tsconfig.json"
	}
}
